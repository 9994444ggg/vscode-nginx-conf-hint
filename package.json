{
    "name": "nginx-conf-hint",
    "displayName": "nginx.conf hint",
    "description": "nginx.conf hint and autocompletion",
    "version": "0.0.1",
    "license": "GPL-3.0",
    "icon": "icon.png",
    "galleryBanner": {
          "color": "#E5FFEC",
          "theme": "light"
    },
    "publisher": "hangxingliu",
    "author": "hangxingliu",
    "engines": {"vscode": "^1.11.0"},
    "categories": ["Snippets","Languages"],
    "activationEvents": ["onLanguage:NGINX"],
    "main": "./extension/main",
    "scripts": {},
    "extensionDependencies": ["shanoor.vscode-nginx"],
    "devDependencies": {"vscode": "^1.11.0"},
    "keywords": ["NGINX", "nginx", "hint", "auto-completion", "configuration"],
    "dependencies": {},
    "contributes": {
          "snippets": [{
                "language": "NGINX",
                "path": "./hint_data/snippets.json"
          }]
    },
    "repository": {
          "type": "git",
          "url": "https://github.com/hangxingliu/vscode-nginx-conf-hint"
    },
    "bugs": {
          "url": "https://github.com/hangxingliu/vscode-nginx-conf-hint/issues"
    },
    "homepage": "https://github.com/hangxingliu/vscode-nginx-conf-hint"
